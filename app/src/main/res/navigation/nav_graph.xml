<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashScreenFragment">
  
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.nda.blum.ui.signup.SignupFragment"
        android:label="login_fragment"
        tools:layout="@layout/signup_fragment" />
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.nda.blum.ui.login.LoginFragment"
        android:label="main_fragment"
        tools:layout="@layout/login_fragment" >
        <action
            android:id="@+id/action_mainFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_mainFragment_to_recuperarPasswordFragment"
            app:destination="@id/recuperarPasswordFragment" />
        <action
            android:id="@+id/action_mainFragment_to_sliderHostFragment"
            app:destination="@id/sliderHostFragment" />
        <action
            android:id="@+id/action_mainFragment_to_hubAlumnoFragment"
            app:destination="@id/hubAlumnoFragment" />
    </fragment>
    <fragment
        android:id="@+id/agendarSesionFragment"
        android:name="com.nda.blum.ui.agendarsesion.AgendarSesionFragment"
        android:label="agendar_sesion_fragment"
        tools:layout="@layout/agendar_sesion_fragment" />
    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.nda.blum.ui.splashscreen.SplashScreenFragment"
        android:label="splash_screen_fragment"
        tools:layout="@layout/splash_screen_fragment" >
        <action
            android:id="@+id/action_splashScreenFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:launchSingleTop="true"
            app:popUpTo="@id/splashScreenFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/slider1Fragment"
        android:name="com.nda.blum.Slider1Fragment"
        android:label="slider1_fragment"
        tools:layout="@layout/slider1_fragment" />
    <fragment
        android:id="@+id/slider2Fragment"
        android:name="com.nda.blum.Slider2Fragment"
        android:label="slider2_fragment"
        tools:layout="@layout/slider2_fragment" />
    <fragment
        android:id="@+id/findingCoachFragment"
        android:name="com.nda.blum.FindingCoachFragment"
        android:label="finding_coach_fragment"
        tools:layout="@layout/finding_coach_fragment" >
        <action
            android:id="@+id/action_findingCoachFragment_to_coachResultFragment"
            app:destination="@id/coachResultFragment" />
    </fragment>
    <fragment
        android:id="@+id/coachResultFragment"
        android:name="com.nda.blum.CoachResultFragment"
        tools:layout="@layout/coach_result_fragment"
        android:label="CoachResultFragment" >
        <action
            android:id="@+id/action_coachResultFragment_to_hubAlumnoFragment"
            app:destination="@id/hubAlumnoFragment" />
    </fragment>
    <fragment
        android:id="@+id/quizzFragment"
        android:name="com.nda.blum.ui.QuizzFragment"
        android:label="quizz_fragment"
        tools:layout="@layout/quizz_fragment" >
        <action
            android:id="@+id/action_quizzFragment_to_findingCoachFragment"
            app:destination="@id/findingCoachFragment" />
    </fragment>
    <fragment
        android:id="@+id/coachBioFragment"
        android:name="com.nda.blum.CoachBioFragment"
        android:label="coach_bio_fragment"
        tools:layout="@layout/coach_bio_fragment" />
    <fragment
        android:id="@+id/coachProfileFragment"
        android:name="com.nda.blum.CoachProfileFragment"
        android:label="coach_profile_fragment"
        tools:layout="@layout/coach_profile_fragment" />
    <fragment
        android:id="@+id/felicidadesCouchFragment"
        android:name="com.nda.blum.FelicidadesCouchFragment"
        android:label="felicidades_couch_fragment"
        tools:layout="@layout/felicidades_couch_fragment" />
    <fragment
        android:id="@+id/recorridoCalendarFragment"
        android:name="com.nda.blum.RecorridoCalendarFragment"
        android:label="RecorridoCalendarFragment"
        tools:layout="@layout/recorrido_calendar_fragment"/>
    <fragment
        android:id="@+id/recorridoChatFragment"
        android:name="com.nda.blum.RecorridoChatFragment"
        android:label="recorrido_chat_fragment"
        tools:layout="@layout/recorrido_chat_fragment" />
    <fragment
        android:id="@+id/couchRecurosFragment"
        android:name="com.nda.blum.CouchRecurosFragment"
        android:label="couch_recuros_fragment"
        tools:layout="@layout/couch_recuros_fragment" />
    <fragment
        android:id="@+id/couchChatsFragment"
        android:name="com.nda.blum.CouchChatsFragment"
        android:label="couch_chats_fragment"
        tools:layout="@layout/couch_chats_fragment" />
    <fragment
        android:id="@+id/coachHorarioFragment"
        android:name="com.nda.blum.CoachHorarioFragment"
        android:label="coach_horario_fragment"
        tools:layout="@layout/coach_horario_fragment" />
    <fragment
        android:id="@+id/sesionersCoachFragment"
        android:name="com.nda.blum.SesionersCoachFragment"
        android:label="sesioners_coach_fragment"
        tools:layout="@layout/sesioners_coach_fragment" />
    <fragment
        android:id="@+id/recursosCoachFragment"
        android:name="com.nda.blum.RecursosCoachFragment"
        android:label="recursos_coach_fragment"
        tools:layout="@layout/recursos_coach_fragment" />
    <fragment
        android:id="@+id/blummersCoachFragment"
        android:name="com.nda.blum.BlummersCoachFragment"
        android:label="blummers_coach_fragment"
        tools:layout="@layout/blummers_coach_fragment" />
  <fragment
        android:id="@+id/sliderHostFragment"
        android:name="com.nda.blum.ui.slider.SliderHostFragment"
        tools:layout="@layout/slider_host_fragment"
        android:label="SliderHostFragment" >
      <action
          android:id="@+id/action_sliderHostFragment_to_quizzFragment"
          app:destination="@id/quizzFragment" />
      <action
          android:id="@+id/action_sliderHostFragment_to_hubAlumnoFragment"
          app:destination="@id/hubAlumnoFragment" />
  </fragment>
    <fragment
        android:id="@+id/hubAlumnoFragment"
        android:name="com.nda.blum.ui.hub.HubAlumnoFragment"
        android:label="hub_alumno_fragment"
        tools:layout="@layout/hub_alumno_fragment" >
        <action
            android:id="@+id/action_hubAlumnoFragment_to_agendarSesionFragment"
            app:destination="@id/agendarSesionFragment" />
        <action
            android:id="@+id/action_hubAlumnoFragment_to_recursosCoachFragment"
            app:destination="@id/recursosCoachFragment" />
        <action
            android:id="@+id/action_hubAlumnoFragment_to_chatWithCoachFragment"
            app:destination="@id/chatWithCoachFragment" />
    </fragment>
    <fragment
        android:id="@+id/recuperarPasswordFragment"
        android:name="com.nda.blum.ui.recoverpassword.RecuperarPasswordFragment"
        tools:layout="@layout/recuperar_password_fragment"
        android:label="RecuperarPasswordFragment" />
    <fragment
        android:id="@+id/chatWithCoachFragment"
        android:name="com.nda.blum.ui.chat.ChatWithCoachFragment"
        tools:layout="@layout/chat_with_coach_fragment"
        android:label="ChatWithCoachFragment" />
  <fragment
        android:id="@+id/nidosCoachFragment"
        android:name="com.nda.blum.NidosCoachFragment"
        android:label="nidos_coach_fragment"
        tools:layout="@layout/nidos_coach_fragment" />
    <fragment
        android:id="@+id/notificacionesCoachFragment"
        android:name="com.nda.blum.NotificacionesCoachFragment"
        android:label="notificaciones_coach_fragment"
        tools:layout="@layout/notificaciones_coach_fragment" />
</navigation>
